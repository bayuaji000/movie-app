{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": "off",
      "prefer-promise-reject-errors": "off",
      "no-use-before-define": "off",
      "no-undef": "off"
    }
  }
  